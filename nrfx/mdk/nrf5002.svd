    <peripheral>
      <name>RAMBIST0</name>
      <description>The RamBist perform hardware accelerated RAM tests</description>
      <baseAddress>0x40025000</baseAddress>
      <headerStructName>RAMBIST</headerStructName>
      <addressBlock>
        <offset>0</offset>
        <size>0x1000</size>
        <usage>registers</usage>
      </addressBlock>
      <groupName>RAMBIST</groupName>
      <size>0x20</size>
      <version>1.0</version>
      <registers>
        <cluster>
          <name>STATUS</name>
          <description>Unspecified</description>
          <headerStructName>RAMBIST_STATUS</headerStructName>
          <access>read-write</access>
          <addressOffset>0x400</addressOffset>
          <register>
            <name>BIST</name>
            <description>Status of the BIST</description>
            <addressOffset>0x000</addressOffset>
            <access>read-only</access>
            <fields>
              <field>
                <name>DONE</name>
                <description>The RamBist is finished</description>
                <lsb>0</lsb>
                <msb>0</msb>
              </field>
              <field>
                <name>PASSED</name>
                <description>The RamBist passed, meaning all the compares in the pattern succeded</description>
                <lsb>1</lsb>
                <msb>1</msb>
              </field>
              <field>
                <name>SLEEP</name>
                <description>The RamBist is currently waiting for a RUNBIST handshake, see retention test protocol</description>
                <lsb>2</lsb>
                <msb>2</msb>
              </field>
            </fields>
          </register>
          <register>
            <name>FAULTY</name>
            <description>Which blocks are failing the test</description>
            <addressOffset>0x004</addressOffset>
            <access>read-only</access>
            <fields>
              <field>
                <name>BLOCKS</name>
                <description>0 if block has no miscompares, 1 if block has miscompares.
 Size and indices are equal to CLAMP.BLOCKS</description>
                <lsb>0</lsb>
                <msb>15</msb>
              </field>
            </fields>
          </register>
        </cluster>
        <register>
          <name>RUN</name>
          <description>Run the BIST engine, must be kept 1 until the BIST is finished</description>
          <addressOffset>0x500</addressOffset>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <fields>
            <field>
              <name>BIST</name>
              <description>When 1, the RamBist runs</description>
              <lsb>0</lsb>
              <msb>0</msb>
            </field>
          </fields>
        </register>
        <cluster>
          <name>CONFIG</name>
          <description>Unspecified</description>
          <headerStructName>RAMBIST_CONFIG</headerStructName>
          <access>read-write</access>
          <addressOffset>0x504</addressOffset>
          <register>
            <name>RAMBIST</name>
            <description>Configuration of the RamBist itself. When none of the fields are set, it runs the standard production test BIST.</description>
            <addressOffset>0x000</addressOffset>
            <access>read-write</access>
            <resetValue>0x00000000</resetValue>
            <fields>
              <field>
                <name>QUICKMODE</name>
                <description>Runs a fast subset of the tests, intended for debug only</description>
                <lsb>0</lsb>
                <msb>0</msb>
              </field>
              <field>
                <name>RETENTION</name>
                <description>Runs the retention test protocol, does only exists if the connected RAMs require retention test</description>
                <lsb>1</lsb>
                <msb>1</msb>
              </field>
            </fields>
          </register>
          <register>
            <name>RAM</name>
            <description>Override of the RAM configuration</description>
            <addressOffset>0x004</addressOffset>
            <access>read-write</access>
            <resetValue>0x00000000</resetValue>
            <fields>
              <field>
                <name>CONFIG</name>
                <description>Depending on the RAM Vendor, this field is different.
 The reset value is also vendor dependent.
(Noted as Vendor[width]: [Signal[width]=..].)
 TSMC0[4:0]: WTSEL[2:0], RTSEL[1:0].
 TSMC1[1:0]: TSEL[1:0].
 MobileSemi[6:0]: BTRIM[3:0], WM, RM, RWM.</description>
                <lsb>0</lsb>
                <msb>6</msb>
              </field>
            </fields>
          </register>
          <register>
            <name>CLAMP</name>
            <description>Clamp/Exclude certain blocks from the test</description>
            <addressOffset>0x008</addressOffset>
            <access>read-write</access>
            <resetValue>0x00000000</resetValue>
            <fields>
              <field>
                <name>BLOCKS</name>
                <description>Bit indexed, if an entry is 1, the block is not receiving stimuli, and excluded from the comparison. Parameterized with the number of RAM macros</description>
                <lsb>0</lsb>
                <msb>15</msb>
              </field>
            </fields>
          </register>
        </cluster>
